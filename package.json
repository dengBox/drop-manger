{
  "name": "drop-manger",
  "version": "0.0.1",
  "description": "2D editor drag tool",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "./index.d.ts",
  "keywords": [
    "vue",
    "javascript",
    "drop"
  ],
  "scripts": {
    "serve": "cross-env NODE_ENV=developemnt rollup -c -w",
    "docs": "cd ./example && yarn serve",
    "build": "rm -fr lib && cross-env NODE_ENV=production rollup -c",
    "build:docs": "cross-env NODE_ENV=developemnt rollup -c",
    "lint": "eslint --fix",
    "test": "jest --coverage",
    "cz": "npm run log && git add . && git cz",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0",
    "postinstall": "node ./script/checkver.js"
  },
  "author": "1716775221@qq.com",
  "license": "MIT OR Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "commitizen": "^4.2.4",
    "commitlint": "^13.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.11",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "jest-html-reporter": "^3.4.1",
    "node-sass": "^4.14.1",
    "postcss-cssnext": "^3.1.1",
    "right-pad": "^1.0.1",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleandir": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3",
    "vue-cli-plugin-commitlint": "^1.0.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "bugs": {
    "url": "https://github.com/dengBox/drop-manger/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dengBox/drop-manger"
  },
  "homepage": "https://dengbox.github.io/drop-manger",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
